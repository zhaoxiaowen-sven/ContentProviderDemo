<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:showIn="@layout/activity_main">

    <Button
        android:id="@+id/bt2"
        android:text="插入一条数据"
        android:onClick="onClick"
        style="@style/button_style" />

    <Button
        android:id="@+id/bt3"
        android:text="删除一条数据"
        android:onClick="onClick"
        style="@style/button_style" />

    <Button
        android:id="@+id/bt4"
        android:text="修改一条数据"
        android:onClick="onClick"
        style="@style/button_style" />

    <Button
        android:id="@+id/bt5"
        android:text="查询一条数据"
        android:onClick="onClick"
        style="@style/button_style" />

    <Button
        android:id="@+id/bt6"
        android:text="查询所有数据"
        android:onClick="onClick"
        style="@style/button_style" />
</merge>